URxvt.font:xft:Roboto Mono:size=12
! URxvt.letterSpace: -5
URxvt.scrollBar: false
! URxvt.transparent: true
! URxvt.blurRadius: 50
! URxvt.cursorColor: #F8F8F2
! URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.internalBorder: 70
URxvt.geometry: 50x25

! Fonts {{{
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.hintstyle: hintfull
Xft.dpi:       120
! }}}

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #1d1f21
*.color8:       #969896

! red
*.color1:       #428095
*.color9:       #428095

! green
*.color2:       #28afb0
*.color10:      #28afb0

! yellow
*.color3:       #f4d35e
*.color11:      #f4d35e

! blue
*.color4:       #f4d35e
*.color12:      #f4d35e

! magenta
*.color5:       #ee964b
*.color13:      #ee964b

! cyan
*.color6:       #ec7357
*.color14:      #ec7357

! white
*.color7:       #c5c8c6
*.color15:      #ffffff

! State:           ´bg´,     ´fg´,     ´bgalt´,  ´hlbg´,   ´hlfg´
rofi.color-normal: #1d1f21,  #c5c8c6,  #313335,  #ec7357,  #1d1f21
!rofi.color-urgent: #1d1f21,  #dc322f,  #eee8d5,  #dc322f,  #fdf6e3
!rofi.color-active: #1d1f21,  #268bd2,  #eee8d5,  #268bd2,  #fdf6e3

!                  ´background´, ´border´, ´separator´
rofi.color-window: #1d1f21,      #ec7357,  #ec7357
