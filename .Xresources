URxvt.font:xft:Roboto Mono:size=12
! URxvt.letterSpace: -5
URxvt.scrollBar: false
! URxvt.transparent: true
! URxvt.blurRadius: 50
! URxvt.cursorColor: #F8F8F2
! URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.internalBorder: 70
URxvt.geometry: 50x25

! Fonts {{{
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.hintstyle: hintfull
Xft.dpi:       120
! }}}

! special
*.foreground:   #ffffff
*.background:   #000000
*.cursorColor:  #ffffff

! black
*.color0:       #000000
*.color8:       #000000

! red
*.color1:       #ffbb00
*.color9:       #ffbb00

! green
*.color2:       #ffbb00
*.color10:      #ffbb00

! yellow
*.color3:       #ffbb00
*.color11:      #ffbb00

! blue
*.color4:       #ffbb00
*.color12:      #ffbb00

! magenta
*.color5:       #ffbb00
*.color13:      #ffbb00

! cyan
*.color6:       #ffbb00
*.color14:      #ffbb00

! white
*.color7:       #aaaaaa
*.color15:      #aaaaaa

! State:           ´bg´,     ´fg´,     ´bgalt´,  ´hlbg´,   ´hlfg´
rofi.color-normal: #000000,  #ffffff,  #111111,  #ffbb00,  #000000
!rofi.color-urgent: #1d1f21,  #dc322f,  #eee8d5,  #dc322f,  #fdf6e3
!rofi.color-active: #1d1f21,  #268bd2,  #eee8d5,  #268bd2,  #fdf6e3

!                  ´background´, ´border´, ´separator´
rofi.color-window: #000000,      #ffbb00,  #ffbb00
