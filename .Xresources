URxvt.font:xft:Roboto Mono:size=12
! URxvt.letterSpace: -5
URxvt.scrollBar: false
! URxvt.transparent: true
! URxvt.blurRadius: 50
! URxvt.cursorColor: #F8F8F2
! URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.internalBorder: 70
URxvt.geometry: 80x25

! Set depth to make transparency work.
URxvt*depth: 32

! Fonts {{{
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.hintstyle: hintfull
Xft.dpi:       120
! }}}

! Colors
*foreground:        #c9c4c4
*background:        #161315
*.foreground:       #c9c4c4
*.background:       #161315
emacs*foreground:   #c9c4c4
emacs*background:   #161315
URxvt*foreground:   #c9c4c4
XTerm*foreground:   #c9c4c4
UXTerm*foreground:  #c9c4c4
URxvt*background:   [100]#161315
XTerm*background:   #161315
UXTerm*background:  #161315
URxvt*cursorColor:  #c9c4c4
XTerm*cursorColor:  #c9c4c4
UXTerm*cursorColor: #c9c4c4
URxvt*borderColor:  [100]#161315

